@model UserDetailViewModel
@{
    Layout = null;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />


<div class="container">
    <br />
    <br />
    <h3>Kullanıcı Profil Bilgileri</h3>
    <div class="alert alert-info">Id : @Model.Id </div>
    <div class="alert alert-warning">Kullanıcı Adı: @Model.UserName </div>
    <div class="alert alert-warning">Ad Soyad : @Model.Name @Model.Surname </div>
    <div class="alert alert-warning">Email Adresi : @Model.Email </div>
</div>